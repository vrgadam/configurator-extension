{
  "name": "AMP Configurator",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Manipulate amp-js configurations locally",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Let's configure!",
    "default_icon": "configurator-icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
