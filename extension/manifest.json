{
  "name": "Getting started",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Injecting stuff",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!",
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
